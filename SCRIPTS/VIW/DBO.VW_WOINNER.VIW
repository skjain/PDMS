CREATE VIEW VW_WOINNER AS 
SELECT JOBOUT.COMP,JOBOUT.UNIT,WONO,WODT,ACCMST.NAME AS PARTY,SUM(JOBOUT.QNTY) AS ISSQNTY,0 AS RECQNTY FROM JOBOUT 
INNER JOIN ACCMST ON JOBOUT.PCOD=ACCMST.CODE 
WHERE RECSTAT<>'D' AND VTYP ='ANX' 
GROUP BY JOBOUT.COMP,JOBOUT.UNIT,WONO,WODT,ACCMST.NAME 
UNION           
SELECT JOBOUT.COMP,JOBOUT.UNIT,WONO,WODT,ACCMST.NAME AS PARTY,0 AS ISSQNTY,SUM(JOBOUT.QNTY) AS RECQNTY FROM JOBOUT 
INNER JOIN ACCMST ON JOBOUT.PCOD=ACCMST.CODE           
WHERE RECSTAT<>'D' AND VTYP='IVR' AND DBCD='000003'    
GROUP BY JOBOUT.COMP,JOBOUT.UNIT,WONO,WODT,ACCMST.NAME